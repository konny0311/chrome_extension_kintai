{
	"name": "Dashboard for クラウド勤怠",
	"manifest_version": 3,
	"version": "0.1",
	"permissions": ["activeTab","scripting", "storage"],
	"action": {
        "default_icon": {
            "32": "/images/chart_icon_32.png"
        },
        "default_popup": "board.html"
    },
    "icons": {
        "32": "/images/chart_icon_32.png"
    },
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Popup the dashboard"
        }
      }    
}